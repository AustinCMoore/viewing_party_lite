<section id="discover">
  <%= button_to "Top Rated Movies", user_movies_path(@user.id), method: :get, params: {top_rated: 'top_rated'} %>

  <%= form_with url: "/users/#{@user.id}/movies", method: :get, local: true do |form| %>
    <%= form.label :search, 'Search For Movie' %>
    <%= form.text_field :search %>
    <%= form.submit 'Search' %>
  <% end %>

</section>